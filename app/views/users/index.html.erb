

<div class="container">
  <div Class="row">

<div class="col-md-3">

 <%= render 'books/list', book: @book, user: @user,user_book: @book %>

 </div>

 <div class="col-md-8 offset-md-1">

<table class="table table-striped">
<h2>Users</h2>
<thead>
    <tr>
      <th>image</th>
      <th>name</th>
     </tr>
</thead>
<tbody>
<% @users.each do |user| %>

 <tr>
 <td> <%= attachment_image_tag user, :profile_image, :fill, 100, 100, format: 'jpeg',fallback: "default-image" %></td>
 <td>  <%= user.name %> </td>
 <td><%= link_to "Show",user_path(user.id) %></td>


</tr>
    <% end %>
    </tbody>
    </table>
  </div>

  </div>
</div>
